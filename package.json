{
  "name": "image-size",
  "version": "0.9.7",
  "description": "fork of get dimensions of any image file",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "./Readme.md",
    "LICENSE"
  ],
  "bin": {
    "image-size": "bin/image-size.js"
  },
  "scripts": {
