{
  "name": "view-image-size",
  "exports": {
    ".": {
      "import": "./lib/mjs/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "types": "./lib/types/index.d.ts",
  "version": "1.0.1",
  "description": "fork of get dimensions of an image file",
  "files": [
    "*/lib/cjs/*",
    "*/lib/mjs/*",
    "*/lib/types/*"
  ],
  "scripts": {
    "build": "npm run build:es6 && npm run build:cjs",
    "build:es6": "tsc -p tsconfig-mjs.json && node ./deploy/copy-mjs.js",
    "build:cjs": "tsc -p tsconfig-cjs.json && node ./deploy/copy-cjs.js",
    "format": "prettier --write \"src/**/*.{ts,js}\"",
    "lint": "prettier --check \"src/**/*.{ts,js}\"",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "clean": "npm run clean:es6 && npm run clean:cjs",
    "clean:es6": "tsc --build --clean tsconfig-mjs.json && node ./deploy/delete-mjs.js",
    "clean:cjs": "tsc --build --clean tsconfig-cjs.json && node ./deploy/delete-cjs.js"
  },
  "keywords": [
    "image",
    "size",
    "dimensions",
    "resolution",
    "width",
    "height",
    "png",
    "jpeg",
    "bmp",
    "gif",
    "psd",
    "webp",
    "svg",
    "icns",
    "ico",
    "cur"
  ],
  "repository": "git://github.com/dyoung2019/view-image-size.git",
  "author": "David Young <dyoung.2019.github@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "del": "^6.0.0",
    "esbuild": "^0.9.7",
    "glob": "^7.1.6",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  }
}
