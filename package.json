{
  "name": "view-image-size",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./dist/index.js"
    }
  },
  "types": "./types/index.d.ts",
  "version": "1.0.1",
  "description": "fork of get dimensions of an image file",
  "files": [
    "dist/*",
    "lib/**",
    "types/*"
  ],
  "scripts": {
    "build": "npm run build:es6 && npm run build:wp && npm run deploy:cjs",
    "build:wp": "webpack",
    "build:es6": "tsc -p tsconfig-mjs.json && node ./deploy/copy-mjs.js",
    "deploy:cjs": "node ./deploy/copy-cjs.js",
    "format": "prettier --write \"src/**/*.{ts,js}\"",
    "lint": "prettier --check \"src/**/*.{ts,js}\"",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "clean": "npm run clean:es6 && npm run clean:lib",
    "clean:lib": "node ./deploy/delete-lib.js",
    "clean:es6": "tsc --build --clean tsconfig-mjs.json",
    "clean:cjs": "tsc --build --clean tsconfig-cjs.json"
  },
  "keywords": [
    "image",
    "size",
    "dimensions",
    "resolution",
    "width",
    "height",
    "png",
    "jpeg",
    "bmp",
    "gif",
    "psd",
    "webp",
    "svg",
    "icns",
    "ico",
    "cur"
  ],
  "repository": "git://github.com/dyoung2019/view-image-size.git",
  "author": "David Young <dyoung.2019.github@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "del": "^6.0.0",
    "esbuild": "^0.9.7",
    "glob": "^7.1.6",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.1.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.2.4",
    "webpack": "^5.33.2",
    "webpack-clean": "^1.2.5",
    "webpack-cli": "^4.6.0"
  }
}
