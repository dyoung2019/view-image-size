{
  "name": "view-image-size",
  "exports": {
    ".": {
      "import": "./lib/mjs/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "types": "./lib/types/index.d.ts",
  "version": "1.0.0",
  "description": "fork of get dimensions of an image file",
  "files": [
    "./lib/*",
    "./Readme.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "snowpack dev",
    "build": "npm run build:es6 && npm run build:cjs && snowpack build",
    "build:es6": "tsc -p tsconfig-mjs.json && node ./deploy/copy-mjs.js",
    "build:cjs": "tsc -p tsconfig-cjs.json && node ./deploy/copy-cjs.js",
    "format": "prettier --write \"src/**/*.{ts,js}\"",
    "lint": "prettier --check \"src/**/*.{ts,js}\"",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "test:wtr": "tsc --build tsconfig-test.json --watch --preserveWatchOutput && wtr specs/*.test.ts --node-resolve --watch",
    "clean": "npm run clean:es6 && npm run clean:cjs",
    "clean:es6": "tsc --build --clean tsconfig-mjs.json && node ./deploy/delete-mjs.js",
    "clean:cjs": "tsc --build --clean tsconfig-cjs.json && node ./deploy/delete-cjs.js"
  },
  "keywords": [
    "image",
    "size",
    "dimensions",
    "resolution",
    "width",
    "height",
    "png",
    "jpeg",
    "bmp",
    "gif",
    "psd",
    "webp",
    "svg",
    "icns",
    "ico",
    "cur"
  ],
  "repository": "git://github.com/dyoung2019/image-size.git",
  "author": "David Young <dyoung.2019.github@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@esm-bundle/chai": "^4.3.4",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "del": "^6.0.0",
    "glob": "^7.1.6",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "snowpack": "^3.1.2",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  }
}
