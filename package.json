  "author": "netroy <aditya@netroy.in> (http://netroy.in/)",
  "license": "MIT",
  "devDependencies": {
    "@snowpack/plugin-typescript": "^1.2.1",
    "prettier": "^2.2.1",
    "snowpack": "^3.1.2",
    "typescript": "^4.2.3"
  }
