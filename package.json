{
  "name": "image-size",
  "exports": {
    ".": {
      "import": "./lib/mjs/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "types": "./lib/types/index.d.ts",
  "version": "0.9.7",
  "description": "fork of get dimensions of any image file",
  "files": [
    "./lib/*",
    "./Readme.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "snowpack dev",
    "build": "npm run build:es6 && npm run build:cjs && snowpack build",
    "build:es6": "tsc -p tsconfig-mjs.json && node ./deploy/copy-mjs.js",
    "build:cjs": "tsc -p tsconfig-cjs.json && node ./deploy/copy-cjs.js",
    "format": "prettier --write \"src/**/*.{ts,js}\"",
    "lint": "prettier --check \"src/**/*.{ts,js}\"",
    "test": "jest",
    "clean": "npm run clean:es6 && npm run clean:cjs",
    "clean:es6": "tsc --build --clean tsconfig-mjs.json && node ./deploy/delete-mjs.js",
    "clean:cjs": "tsc --build --clean tsconfig-cjs.json && node ./deploy/delete-cjs.js",
    "es6": "node --experimental-modules ./archive/demo.mjs",
    "cjs": "node ./enc.js"
  },
  "keywords": [
    "image",
    "size",
    "dimensions",
    "resolution",
    "width",
    "height",
    "png",
    "jpeg",
    "bmp",
    "gif",
    "psd",
    "webp",
    "svg",
    "icns",
    "ico",
    "cur"
  ],
  "repository": "git://github.com/dyoung2019/image-size.git",
  "author": "David Young <tgsstdio@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@snowpack/plugin-typescript": "^1.2.1",
    "del": "^6.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "snowpack": "^3.1.2",
    "typescript": "^4.2.3"
  }
}
